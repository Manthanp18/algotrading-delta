{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DELTA_API_KEY": "@delta_api_key",
    "DELTA_API_SECRET": "@delta_api_secret",
    "DELTA_BASE_URL": "@delta_base_url"
  },
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}